{
  "name": "munny",
  "version": "1.0.0",
  "scripts": {
    "start": "node build/scripts/debug.js",
    "build": "npm run pulp-build && npm run webpack-build && npm run clear-assets && npm run copy-assets",
    "pulp-build": "pulp build --to output/pulp_bundle.js",
    "webpack-build": "webpack --config build/config/webpack.prod.config.js",
    "clear-assets": "rm -rf ../native/munny-android/app/src/main/assets/app",
    "copy-assets": "mkdir ../native/munny-android/app/src/main/assets/app && cp -R dist/* ../native/munny-android/app/src/main/assets/app"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "psc-package": "^0.3.2",
    "pulp": "^12.3.0",
    "purescript": "^0.12.0",
    "webpack": "^4.18.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  }
}
